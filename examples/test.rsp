(def width  500)
(def height 500)

(canvas width height)

(def gradient
  (-> (img-load   "examples/assets/grad_b2w_100x100.png")
      (img-resize 'bilinear width height)
      ))

(def image
  (-> (img-load   "examples/assets/mh_lena_2048.png")
      (img-resize 'bilinear width height)
      (img-mix    gradient 'multiply)
      ))

(img-render image)